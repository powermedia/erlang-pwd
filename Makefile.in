
all: compile

compile:
	test -d ebin || mkdir ebin
	cd priv && make

debug:
	cd priv && make debug

clean:
	rm -rf ./ebin/*.so

docs:
	@ERL@ -noshell -run edoc_run application "'pwd'" '"."' '[{title, "Erlang binding for Posix User Database Access"}, {hidden, false}, {private, false}]' -s erlang halt
